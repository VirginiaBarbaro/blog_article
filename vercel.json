{
  "version": 2,
  "builds": [
    { "src": "./server.js", "use": "@vercel/node" },
    { "src": "./public/**/*", "use": "@vercel/static" },
    { "src": "./views/**/*", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "public": "public",
  "includeFiles": [
    "views/**/*"
  ]
  
}